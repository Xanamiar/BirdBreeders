<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jwknight.birdbreedersdiary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".BirdBreeder"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Budgies"
            android:label="@string/title_activity_budgies"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Flock"
            android:label="@string/title_activity_flock"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddBird"
            android:label="@string/title_activity_add_bird"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Mutations"
            android:label="@string/title_activity_mutations"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
